{
  "name": "iflux2",
  "version": "1.4.1",
  "description": "Reactive state container (based on immutable) for React or ReactNative, inspired by mapreduce.",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --verbose",
    "build:core": "node_modules/.bin/babel --ignore src/contrib src --out-dir dist",
    "build:contrib": "node_modules/.bin/babel src/contrib --out-dir contrib",
    "flowtype": "cp types/index.js.flow dist && cp types/atom.js.flow contrib && cp types/validator.js.flow contrib && cp types/util.js.flow contrib",
    "release": "npm run build:core && npm run build:contrib && npm run flowtype"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/examples/"
    ]
  },
  "author": "hufeng",
  "license": "BSD",
  "dependencies": {
    "events": "^1.1.0",
    "immutable": "^3.8.1",
    "object-assign": "^4.1.1"
  },
  "files": [
    "README.md",
    "dist",
    "contrib"
  ],
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-jest": "^18.0.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.16.0",
    "jest-cli": "^18.1.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-test-renderer": "^15.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/QianmiOpen/iflux2.git"
  },
  "keywords": [
    "Reactive",
    "state",
    "React",
    "ReactNative"
  ]
}
